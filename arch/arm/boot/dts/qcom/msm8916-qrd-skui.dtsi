			*/
			compatible = "awinic,aw2013";
			reg = <0x45>;
			vdd-supply = <&pm8916_l17>;
			vcc_i2c-supply = <&pm8916_l6>;
			awinic,ledr-parameter_array = <3 0 3 0 5 0 0>;
			awinic,ledg-parameter_array = <3 0 3 0 5 0 0>;
			awinic,ledb-parameter_array = <3 0 3 0 5 0 0>;
		};
};

