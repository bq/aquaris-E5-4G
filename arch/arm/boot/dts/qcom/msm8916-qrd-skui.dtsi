&spmi_bus {
        qcom,pm8916@0 {
                qcom,leds@a100 {
                        status = "okay";
                        qcom,led_mpp_2 {
                                label = "mpp";
                                linux,name = "button-backlight";
                                linux,default-trigger = "none";
                                qcom,default-state = "off";
                                qcom,max-current = <20>;
                                qcom,current-setting = <20>;
                                qcom,id = <6>;
                                qcom,mode = "manual";
                                qcom,source-sel = <1>;
                                qcom,mode-ctrl = <0x60>;
                        };
                };
        };
};

&sdhc_2 {
	qcom,nonremovable;

	interrupts = <0 1>;
	interrupt-map = <0 &intc 0 125 0
		1 &intc 0 221 0>;
	interrupt-names = "hc_irq", "pwr_irq";
	
	qcom,clk-rates = <400000 15000000>;
	/delete-property/ cd-gpios;
};

